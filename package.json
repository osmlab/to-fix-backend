{
  "name": "to-fix-backend",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "precommit": "lint-staged",
    "test": "npm run lint && NODE_ENV=test tape test/*.test.js | tap-colorize",
    "lint": "eslint .",
    "format": "prettier --single-quote --write \"{,test/**/}*.{js,json}\"",
    "api-docs":
      "documentation readme ./lib/server.js -f md --readme-file API.md --section Endpoints --shallow",
    "setup-database": "node bin/setup-database.js"
  },
  "author": "Mapbox",
  "license": "BSD",
  "dependencies": {
    "@mapbox/geojsonhint": "^2.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.5",
    "fastlog": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.4",
    "mapbox-error": "^2.2.0",
    "memory-cache": "^0.2.0",
    "nock": "^9.0.22",
    "ohauth": "^1.0.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prettier": "^1.7.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "^4.10.0",
    "xml2json": "^0.11.0"
  },
  "engines": {
    "node": ">=6.11.x"
  },
  "devDependencies": {
    "documentation": "^5.3.2",
    "eslint": "^2.8.0",
    "proxyquire": "^1.8.0",
    "supertest": "^3.0.0",
    "tap-colorize": "^1.2.0",
    "tape": "^4.6.2"
  },
  "lint-staged": {
    "*.json": ["prettier --single-quote --write", "git add"],
    "*.js": [
      "eslint .",
      "prettier --single-quote --write",
      "npm run api-docs",
      "git add"
    ]
  }
}
